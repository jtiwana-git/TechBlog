{{#each blogs as |blog| }}
<div class="row mb-4 blog">
  <div class="col-md-5">
    <h2 class="blogListTitle">
      <a href="/blogs/{{blog.id}}">{{blog.username}}</a>
    </h2>
    <p>Created by {{user.username}} on {{blog.date_created}}</p>
  </div>
  <div class="col-md-7">
    <h2>{{title}}</h2>
    <p>
      {{blog.content}}
    </p>
  </div>
</div>
{{/each}}